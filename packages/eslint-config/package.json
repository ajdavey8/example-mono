{
  "name": "@andrew-org-test/eslint-config",
  "author": "Andrew",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "Library for backend eslint config and custom rules",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrew-org-test/optimus.git",
    "directory": "packages/eslint-config"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "pkgRoot": "packages/eslint-config/"
  },
  "files": [
    "dist/src/**"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "cz",
    "compile": "tsc --build",
    "lint": "echo \"No linting configured\"",
    "lint:fix": "echo \"No linting configured\"",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release --extends=./release.config.js"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "eslint": ">=8.47.0",
    "eslint-config-prettier": ">=8.8.0",
    "eslint-plugin-import": ">=2.26.0",
    "eslint-plugin-jest": ">=27.2.3"
  },
  "devDependencies": {
    "@nx/js": "16.1.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.17.5",
    "@typescript-eslint/utils": "6.4.1",
    "commitizen": "4.3.0",
    "conventional-changelog-conventionalcommits": "6.0.0",
    "cz-conventional-changelog": "3.3.0",
    "jest": "29.6.2",
    "nx": "16.1.3",
    "prettier": "3.0.2",
    "semantic-release": "21.0.7",
    "semantic-release-monorepo": "7.0.5",
    "semantic-release-slack-bot": "4.0.2",
    "semantic-release-yarn": "2.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
