{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "sourceRoot": "/",
      "inlineSources": true,
      "strict": true,
      "target": "es2017",
      "outDir": "dist"
    }
  }
    